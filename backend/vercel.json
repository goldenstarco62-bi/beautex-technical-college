{
    "version": 2,
    "functions": {
        "src/server.js": {
            "runtime": "nodejs22.x"
        }
    },
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}